### MASTER TPM will only be used if the env. variable TPM_SERVER_NAME
### and TPM_SERVER_PORT are not set.
MASTER_TPM_PORT=6544
MASTER_TPM_SERVER=localhost
MASTER_TPM_NOTIFY_PORT=6546

if [ -z $SLAVE_TPM_PORT ]; then
    SLAVE_TPM_PORT=6545
fi
if [ -z $SLAVE_TPM_SERVER ]; then
    SLAVE_TPM_SERVER=localhost
fi

